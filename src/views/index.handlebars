<div id="carouselHome" class="carousel slide" data-ride="carousel">
  <ol class="carousel-indicators">
    {{#each slides}}
    <li data-target="#carouselHome" data-slide-to="{{this.idx}}" class="{{#if this.status}} active{{/if}}"></li>
    {{/each}}
  </ol>
  <div class="carousel-inner">
    {{#each slides}}
    <div class="carousel-item {{#if this.status}} active{{/if}} ">
      <img src="/assets/uploads/{{this.photo}}" class="d-block w-100 img-fluid" alt="..." style="height: 750px;">
      <div class="carousel-caption d-none d-md-block">
        <h5>{{this.title}}</h5>
        <p>{{this.sub}}</p>
      </div>
    </div>
    {{/each}}
  </div>
  <a class="carousel-control-prev" href="#carouselHome" role="button" data-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="carousel-control-next" href="#carouselHome" role="button" data-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
</div>

<div id="areas-home">
  <div class="container">
    <div class="row">
      <div class="col-lg-12  p-4">
        <h2>{{lang.home.service_session_title}}</h2>
        <p class="text-center">
          {{office.service}}
        </p>
      </div>
      <div class="col-lg-12 box-specs justify-content-center">
        <div class="row ">
          {{#each services}}
          <div class="col-md-12 col-xl-4">

            <div class="box-spec ">
              <a href="/areas-de-consultorias/{{this._id}}"
                style="text-decoration: none; color: white;">{{this.name}}</a>
            </div>

          </div>
          {{/each}}
        </div>
      </div>
    </div>
  </div>
</div>

<div class="parceiros">
  <h2>{{lang.home.partner_session_title}}</h2>
  <div class="container">
    <div class="all-partners">
      {{#each partners}}
      <div class="parceiro">
        <a href="">
          <img src="assets/uploads/{{this.photo}}" alt="{{this.name}}">
        </a>
      </div>
      {{/each }}
    </div>
  </div>
</div>

<br>

<div class="parceiros">
  <h2>{{lang.home.certification_session_title}}</h2>
  <div class="container">
    <div class="all-partners">
      {{#each certifications}}
      <div class="parceiro">
        <a href="#!">
          <img src="assets/uploads/{{this.photo}}" height="100" alt="{{this.name}}">
        </a>
      </div>
      {{/each }}
    </div>
