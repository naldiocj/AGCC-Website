        <a href="/product/{{this._id}}">
          <img src="assets/uploads/{{this.photo}}" width="100%" height="200" alt="Avatar" />
        </a><br />
        <span style="font-weight: bold;font-size: 18px;">{{this.name}}</span>
      </div>
